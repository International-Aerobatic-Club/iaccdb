<p id="notice"><%= notice %></p>

<h1><%= @contest.name %></h1>
<p class="contest_place">Held in <%= @contest.place %>, <%= @contest.start %></p>
<p class="contest_director">Directed by <%= @contest.director %>, 
   Chapter <%= @contest.chapter %></p>

<% @c_results.each do |c_result| %>
  <%= render :template => 'c_results/category',
    :locals => { :c_result => c_result, 
      :judges => @judges[c_result] } %>
<% end %>
